{
	"name": "colornamify-code",
	"displayName": "colornamify-code",
	"description": "Thinks of names for colors.",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "colornamify.copy",
				"title": "Copy current Color Name"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"eslint": "^4.11.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"clipboardy": "^1.2.3",
		"colornamify": "^1.0.1",
		"pigment": "^0.1.0"
	}
}
